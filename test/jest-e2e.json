{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "./",
  "modulePaths": [
    "<rootDir>"
  ],
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@cheetah/dtos/(.*)": "<rootDir>/../libs/dtos/src/$1",
    "@cheetah/dtos": "<rootDir>/../libs/dtos/src",
    "@cheetah/error-handler/(.*)": "<rootDir>/../libs/error-handler/src/$1",
    "@cheetah/error-handler": "<rootDir>/../libs/error-handler/src",
    "@cheetah/logger/(.*)": "<rootDir>/../libs/logger/src/$1",
    "@cheetah/logger": "<rootDir>/../libs/logger/src",
    "@cheetah/constants/(.*)": "<rootDir>/../libs/constants/src/$1",
    "@cheetah/constants": "<rootDir>/../libs/constants/src",
    "@cheetah/common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@cheetah/common": "<rootDir>/../libs/common/src"
  }
}